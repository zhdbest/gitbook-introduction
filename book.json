{
    "title" : "测试gitbook",
    "language" : "zh-hans",
    "style" : {
        "website": "styles/website.css"
    },
    "plugins" : [
        "github",
        "-sharing",
        "-livereload",
        "code",
        "popup",
        "-search", 
        "-lunr",
        "search-pro",
        "ancre-navigation",
        "alerts",
        "flexible-alerts",
        "advanced-emoji"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/zhdbest"
        },
        "flexible-alerts" : {
            "style": "flat",
            "note" : {
                "label" : "笔记"
            },
            "comment": {
                "label": "评论",
                "icon": "fa fa-comments",
                "className": "info"
            }
        }
    }
}